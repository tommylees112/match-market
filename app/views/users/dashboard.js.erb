<% if params[:b] %>
  var boughtContainer = document.getElementById('booking-container');
  boughtContainer.innerHTML = "<%= j render 'shared/dashboard_bookings_container', user_bookings: @user_bookings %>"
<% elsif params[:s] %>
  var soldContainer = document.getElementById('sold-odds-container');
  soldContainer.innerHTML = "<%= j render 'shared/dashboard_sold_odds_container', created_odds: @created_odds %>"
<% end %>
